algoritmo "notas"
var
   nota1, nota2, notaFinal : real
inicio
   escreva("Dgite a primeira nota: ")
   leia(nota1)
   escreva("Digite a segunda nota: ")
   leia(nota2)
   
   notaFinal <- (nota1 + nota2)

   escreval("NOTA FINAL = ", notafinal)
   
   se notaFinal < 60 entao
     escreval("REPROVADO")
   fimse
fimalgoritmo